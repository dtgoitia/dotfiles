	"dynamic upper back (rope)": {
		"prefix": "upper core (rope)",
		"body": [
			"  <preset desc=\"2020-$1\" simple=\"0\" summary=\"dynamic upper back (rope)\">",
			"    <btimer desc=\"Prepare rope on the door\" pause=\"1\" time=\"3000\" work=\"0\" />",
			"    <btimer desc=\"Rotate arms to warm up shoulders\" pause=\"0\" time=\"30000\" work=\"1\" />",
			"    <btimer desc=\"Warm up elbows\" pause=\"0\" time=\"20000\" work=\"1\" />",
			"    <btimer desc=\"Now push-ups. Get ready\" pause=\"1\" time=\"3000\" work=\"0\" />",
			"    <loop desc=\"Push-ups\" rounds=\"3\">",
			"      <btimer desc=\"10 push-ups\" pause=\"1\" time=\"3000\" work=\"1\" />",
			"      <btimer desc=\"Rest\" pause=\"0\" time=\"20000\" work=\"0\" />",
			"    </loop>",
			"    <btimer desc=\"Now inverted rowing. Get ready\" pause=\"1\" time=\"3000\" work=\"0\" />",
			"    <loop desc=\"Inverted rows\" rounds=\"3\">",
			"      <btimer desc=\"10 inverted rows\" pause=\"1\" time=\"3000\" work=\"1\" />",
			"      <btimer desc=\"6 biceps inverted rows\" pause=\"0\" time=\"20000\" work=\"1\" />",
			"    </loop>",
			"    <btimer desc=\"Now abdominals. Get ready\" pause=\"1\" time=\"3000\" work=\"0\" />",
			"    <loop desc=\"Abs\" rounds=\"3\">",
			"      <btimer desc=\"Hold\" pause=\"0\" time=\"15000\" work=\"1\" />",
			"      <btimer desc=\"Rest\" pause=\"0\" time=\"5000\" work=\"0\" />",
			"    </loop>",
			"  </preset>",
		],
		"description": "dynamic upper back (rope)"
	},
