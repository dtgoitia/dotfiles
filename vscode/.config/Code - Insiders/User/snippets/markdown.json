			"  - ?",
			"",
			"## Prior to meeting",
			"",
			"...",
			"",
			"## Meeting invite",
			"",
			"...",
			"",
			"## Meeting minutes",
			"",
			"<!-- External references -->",
			"",
			"[1]: ___ \"___\"",
		],
		"description": "Pull request record for the job log."
